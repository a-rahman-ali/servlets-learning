<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>JEE3</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>requestDispatcher.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>callProgram1</servlet-name>
  	<servlet-class>com.servlets.Program1</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>callProgram1</servlet-name>
  	<url-pattern>/call1</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>callProgram2</servlet-name>
  	<servlet-class>com.servlets.Program2</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>callProgram2</servlet-name>
  	<url-pattern>/call2</url-pattern>
  </servlet-mapping>
  
</web-app>